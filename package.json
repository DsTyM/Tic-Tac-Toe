{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "description": "Tic Tac Toe Game",
  "main": "main.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "yourappid",
    "category": "your.app.category.type",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DsTyM/Tic-Tac-Toe.git"
  },
  "author": "DsTyM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DsTyM/Tic-Tac-Toe/issues"
  },
  "homepage": "https://github.com/DsTyM/Tic-Tac-Toe#readme",
  "devDependencies": {
    "electron": "^8.1.1",
    "electron-builder": "^22.4.1"
  }
}
